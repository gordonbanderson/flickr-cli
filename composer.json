{
    "name": "thefox/flickr-cli",
    "description": "Upload and download Flickr photos, photo sets, directories via shell.",
    "license": "GPL-3.0",
    "type": "project",
    "keywords": [
        "Flickr",
        "Upload",
        "Download",
        "Photo",
        "CLI"
    ],
    "homepage": "https://github.com/TheFox/flickr-cli",
    "authors": [
        {
            "name": "Christian Mayer",
            "email": "christian@fox21.at",
            "homepage": "https://fox21.at"
        }
    ],
    "require": {
        "php": "^7.0",
        "rezzza/flickr": "dev-ss4",
        "symfony/yaml": "^2.3",
        "symfony/console": "^4",
        "symfony/filesystem": "^4",
        "symfony/finder": "^3.1",
        "monolog/monolog": "^1.21",
        "guzzlehttp/guzzle": "^6",
        "lusitanian/oauth": "^0",
        "rych/bytesize": "^1.0",
        "doctrine/dbal": "^2.5",
        "doctrine/annotations": "^1",
        "doctrine/cache": "^1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "TheFox\\": "src"
        }
    },
    "bin": [
        "bin/flickr-cli"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true

}
